{
    "swagger": "2.0",
    "info": {
        "version": "1.0.0", 
        "title": "My User Project CRUD"
    },
    "host": "localhost:5000",
    "basePath": "/",
    "tags": [
        {
            "name": "API",
            "description": "API for users in the system"
        }
    ],
    "schemes": [
        "http"
    ],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/users/{userId}": {
             "get": {
                "tags": [
                    "User"
                ],
                "summary": "Get one user of sistem",
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "ID of user to return",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/Users"
                        }
                    }
                }
            }
        }   
    },
    "definitions": {
        "Users": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            }
        }
    }
}